services:
  shopping.client:
    build:
      context: /Users/khaledallamabdalla/Documents/dev/learn/microservices/DotNetMicroservicesWithK8s/zTest/shopping
      dockerfile: Shopping.Client/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Shopping.Client
      target: base
    container_name: Shopping.Client
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: shopping:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/Shopping.Client.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - /Users/khaledallamabdalla/Documents/dev/learn/microservices/DotNetMicroservicesWithK8s/zTest/shopping/Shopping.Client:/app:rw
    - /Users/khaledallamabdalla/vsdbg/vs2017u5:/remote_debugger:rw
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/khaledallamabdalla/.nuget/packages:/root/.nuget/packages:ro
    - /Users/khaledallamabdalla/Documents/dev/learn/microservices/DotNetMicroservicesWithK8s/zTest/shopping:/src:rw
version: '3.4'