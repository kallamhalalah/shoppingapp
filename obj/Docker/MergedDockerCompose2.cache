services:
  shopping.client:
    build:
      context: /Users/khaledallamabdalla/Documents/dev/learn/microservices/DotNetMicroservicesWithK8s/zTest/shopping
      dockerfile: Shopping.Client/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Shopping.Client
    container_name: Shopping.Client
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: shopping
    labels:
      com.microsoft.visual-studio.project-name: Shopping.Client
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/Shopping.Client.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - /Users/khaledallamabdalla/vsdbg/vs2017u5:/remote_debugger:rw
version: '3.4'